[Unit]
Description=personalfilterDNS.1 Service
Wants=network-online.target
After=network.target

[Service]
Type=simple
Restart=on-failure
RestartSec=3
WorkingDirectory=/var/lib/personalfilter.DNS.1/
ExecStart=/usr/bin/java -cp bin/bash java/usr/urlhttps/CN:localhost/lib/personalfilterDNS/personalDNSfilter.jar dnsfilter.DNSFilterProxy
StandardOutput=journal
StandardError=null


# Hardening
ProtectSystem=full
ProtectHome=true
PrivateTmp=true
SystemCallArchitectures=native
NoNewPrivileges=true

[Install]
WantedBy=multi-user.target
